<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="sysdate" dbms="oracle"/>
    <property name="now" value="now()" dbms="mysql,hsqldb,postgresql,h2"/>
    <property name="now" value="CURRENT_TIMESTAMP" dbms="mssql"/>
    <property name="dateTimeNoTimeZone" value="TIMESTAMP WITHOUT TIME ZONE" dbms="h2,postgresql"/>

    <property name="uuid_function" value="UUID_GENERATE_V4()" dbms="postgresql"/>
    <property name="uuid_function" value="RANDOM_UUID()" dbms="h2"/>
    <property name="now_utc" value="(now() at time zone 'UTC')" dbms="postgresql"/>
    <property name="now_utc" value="SYSDATE" dbms="h2"/>
    <property name="schema" value="public"/>
</databaseChangeLog>
